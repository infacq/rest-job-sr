{
	"private": true,
	"devDependencies": {
		"browserify": "^10.2.4",
		"browserify-shim": "^3.8.8",
		"gulp": "^3.8.8",
		"laravel-elixir": "*"
	},
	"dependencies": {
		"angular": "^1.3.16",
		"angular-material": "angular/bower-material",
		"ng-file-upload": "^5.0.4"
	},
	"browser": {
		"angular": "./node_modules/angular/angular.js",
		"ng-file-upload": "./node_modules/ng-file-upload/dist/ng-file-upload-all.js"
	},
	"browserify-shim": {
		"angular": {
			"exports": "angular"
